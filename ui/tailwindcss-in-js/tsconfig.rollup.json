{
  "compilerOptions": {
      "moduleResolution": "node",
      "jsx": "react",
      "target": "es5",
      "module": "esnext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "outDir": "./dist",
      "noImplicitAny": true,
  }
}
